{
  "name": "aioli",
  "version": "0.2.3",
  "description": "vaul-like, radix-vue based bottomsheet/drawer component",
  "author": "Maik Kowol<maik.s.kowol@gmail.com>",
  "license": "MIT",
  "homepage": "https://94726.github.io/aioli",
  "repository": {
    "type": "git",
    "url": "https://github.com/94726/aioli"
  },
  "bugs": {
    "url": "https://github.com/94726/aioli/issues"
  },
  "keywords": [
    "vue",
    "drawer",
    "bottom sheet",
    "dialog",
    "drawer",
    "vaul"
  ],
  "type": "module",
  "main": "dist/index.js",
  "module": "./dist/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./nuxt": {
      "import": "./dist/nuxt.js",
      "types": "./dist/nuxt.d.ts"
    },
    "./styles": {
      "import": "./dist/styles.css"
    },
    "./dist/*": "./dist/*"
  },
  "scripts": {
    "build": "vite build",
    "typecheck": "vue-tsc --noEmit -p .",
    "prepack": "cp ../README.md ./",
    "postpack": "rm ./README.md"
  },
  "devDependencies": {
    "@nuxt/schema": "~3.12.4",
    "@vitejs/plugin-vue": "^5.1.2",
    "@vueuse/core": "^10.11.0",
    "radix-vue": "1.9.2",
    "vite": "^5.3.5",
    "vite-plugin-dts": "^3.9.1",
    "vue": "^3.4.35"
  },
  "dependencies": {
    "@nuxt/kit": "~3.12.4"
  },
  "peerDependencies": {
    "radix-vue": "1.x",
    "vue": "3.x"
  }
}
